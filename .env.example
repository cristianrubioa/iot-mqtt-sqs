
# AWS MQTT Configuration
AWS_MQTT__ENDPOINT=your-iot-endpoint.iot.us-east-1.amazonaws.com
AWS_MQTT__PORT=8883
AWS_MQTT__TOPIC=sdk/test/python
AWS_MQTT__KEEPALIVE=60
AWS_MQTT__ROOT_CA=certs/your-AmazonRootCA1.pem
AWS_MQTT__CERT_FILE=certs/your-device-certificate.pem.crt
AWS_MQTT__PRIVATE_KEY=certs/your-private.pem.key
AWS_MQTT__CLIENT_ID=basicPubSub
AWS_MQTT__QOS=1

# Sensor Configuration
SENSOR__DEVICE_ID=sensor-001
SENSOR__INTERVAL=5
SENSOR__RANDOM_SEED=42

# General Settings
LOG_LEVEL=INFO

